<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/user_page"
    tools:context=".existingUser">

    <Button
        android:id="@+id/seeDC"
        android:layout_width="302dp"
        android:layout_height="31dp"
        android:layout_marginTop="248dp"
        android:background="@drawable/challenge_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/iCT"
        android:layout_width="350dp"
        android:layout_height="47dp"
        android:layout_marginTop="28dp"
        android:background="@drawable/invest"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seeDC" />

    <Button
        android:id="@+id/LOCK"
        android:layout_width="232dp"
        android:layout_height="45dp"
        android:layout_marginStart="66dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="67dp"
        android:layout_marginBottom="256dp"
        android:background="@drawable/lock_button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iCT" />

</androidx.constraintlayout.widget.ConstraintLayout>